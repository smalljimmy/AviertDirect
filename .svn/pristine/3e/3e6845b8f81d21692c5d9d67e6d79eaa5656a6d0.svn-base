table {
   font-size: 13px; 
}

.ui-dialog { 
   padding: 2px; }

.ui-dialog .ui-dialog-content { 
   background-color: rgb(198,178,167); }

.description { 
   font-family: 'OpenSansRegular'; 
   font-weight: normal;
   font-style: normal; 
   font-size: 13px; 
   padding: 8px 0;
   clear: both; }
   
